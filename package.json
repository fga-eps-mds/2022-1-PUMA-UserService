  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:comm": "jest --coverage test/comunidade/comunidade.controller.spec.ts",
    "test:maps": "jest --coverage test/mapas/mapas.controller.spec.ts",
    "test:media": "jest --coverage test/midia/midia.controller.spec.ts",
    "test:users": "jest --coverage test/users/users.controller.spec.ts",
    "test:firebase": "jest --coverage test/firebase/firebase.controller.spec.ts",
    "test:cov": "jest --coverage --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint . --fix"
