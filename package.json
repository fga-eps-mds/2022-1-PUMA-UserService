    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mocha-junit-reporter": "^2.0.2",
    "nodemailer": "^6.7.2",
    "nyc": "^15.1.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
